@model IEnumerable<EndlasNet.Data.OptionalLaserService>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HeatTreatedBlankWt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HeatTreatedPricePerLb)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinHeatTreatmentPrice)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HeatTreatedBlankWt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HeatTreatedPricePerLb)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinHeatTreatmentPrice)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OptionalLaserServicesId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OptionalLaserServicesId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OptionalLaserServicesId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
