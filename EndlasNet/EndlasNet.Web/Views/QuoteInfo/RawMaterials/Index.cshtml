@model IEnumerable<EndlasNet.Data.RawMaterial>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PowderType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PowderLayerPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CladLayerDensity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vendor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PowderFeeder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RawMaterialEmpirical)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PowderType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PowderLayerPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CladLayerDensity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vendor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PowderFeeder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RawMaterialEmpirical.RawMaterialEmpiricalId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RawMaterialId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RawMaterialId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RawMaterialId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
