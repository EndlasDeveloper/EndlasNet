@model IEnumerable<EndlasNet.Data.QuoteSession>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuoteSessionName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuoteSessionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Customer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuoteSessionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuoteSessionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer.Address)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.QuoteSessionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.QuoteSessionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.QuoteSessionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
