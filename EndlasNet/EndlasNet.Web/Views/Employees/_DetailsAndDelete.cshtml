@model EndlasNet.Data.Employee


<div>
    <h4>Employee</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            First name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            Last name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class="col-sm-2">
            Endlas email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EndlasEmail)
        </dd>
    </dl>
</div>