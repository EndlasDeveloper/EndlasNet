@model IEnumerable<EndlasNet.Data.Insert>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PurchaseOrderNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PurchaseOrderDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PurchaseOrderPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorPartNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ToolTipRadius)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PurchaseDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vendor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PurchaseOrderNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PurchaseOrderDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PurchaseOrderPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorPartNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ToolTipRadius)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PurchaseDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vendor.PointOfContact)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InsertId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InsertId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InsertId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
