@model EndlasNet.Data.EnvironmentalSnapshot

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Environmental snapshot</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
         Snapshot date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateTimeCollected)
        </dd>
        <dt class = "col-sm-2">
            Temperature
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Temperature)
        </dd>
        <dt class = "col-sm-2">
            Humidity
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Humidity)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.EnvSnapshotId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
