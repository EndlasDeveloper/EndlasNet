@model EndlasNet.Data.WorkItem

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Work item</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Work Endlas number
        </dt>
       <dd class="col-sm-10">
           @Html.DisplayFor(model => model.Work.EndlasNumber)
       </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.WorkItemId">Edit</a> |
    <a asp-action="Index" asp-route-workId="@Model.WorkId">Back to list</a>
</div>
