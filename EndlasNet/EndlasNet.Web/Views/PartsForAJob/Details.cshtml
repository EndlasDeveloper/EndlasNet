@model EndlasNet.Data.PartForJob

@{
    ViewData["Title"] = "Details";
    string imageName = "NA";
    string imageUrl = "NA";
    if (Model.PartForWorkImg != null)
    {
        imageName = Model.PartForWorkImg.ImageName;
        imageUrl = Model.PartForWorkImg.ImageUrl;
    }
}

<h1>Details</h1>

<div>
    @await Html.PartialAsync("_DetailsAndDelete", Model)
</div>
<br />

<div>
    <a asp-action="Edit" asp-route-id="@Model.PartForWorkId">Edit</a> |
    <a asp-action="BackToPartBatch" asp-route-workItemId="@Model.WorkItemId">Back to list</a>
</div>
