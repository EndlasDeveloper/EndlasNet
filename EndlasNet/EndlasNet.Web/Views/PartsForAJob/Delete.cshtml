@model EndlasNet.Data.PartForJob

@{
    ViewData["Title"] = "Delete";
   
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    @await Html.PartialAsync("_DetailsAndDelete", Model)
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="PartForWorkId" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    @Html.ActionLink("Back to list", "BackToList", new { id = Model.PartForWorkId, workId = Model.WorkId, partInfoId = Model.StaticPartInfoId })
</form>
